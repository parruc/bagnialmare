#: templates/account/email_confirm.html:17
#, python-format
msgid ""
"Please confirm that <a href=\"mailto:%(email)s\">%(email)s</a> is an e-mail "
"address for user %(user_display)s."
msgstr ""
"Per favore conferma di esserti registrato con l'indirizzo email <a href="
"\"mailto:%(email)s\">%(user_display)s</a>"

#: templates/account/email_confirm.html:21
msgid "Confirm"
msgstr "Conferma"

#: templates/account/email_confirm.html:28
#, python-format
